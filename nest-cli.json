{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": false,
    "assets": [
      {"include": "../config/*.yaml",
        "outDir": "./dist/config"},
      {"include":  "../config/ormconfig.json",
        "outDir": "./dist"}
    ]
  }
}
